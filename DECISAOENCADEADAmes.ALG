Algoritmo "semnome"
// Disciplina   : [Linguagem e L�gica de Programa��o]
// Professor   : Jailson
// Descri��o   : estrutura encadeada 
// Autor(a)    : Kauan
// Data atual  : 05/09/2024
Var
   // Se��o de Declara��es das vari�veis
   mes: inteiro

Inicio
   // Se��o de Comandos, procedimento, fun��es, operadores, etc...
   escreval ("digite o numero de um mes")
   leia (mes)
   se mes = 1 entao
      escreval ("janeiro")
   senao
      se mes = 2 entao
         escreval ("fevereiro")
      senao
         se mes = 3 entao
            escreval ("marco")
         senao
            se mes = 4 entao
               escreval ("abril")
            senao
               se mes = 5 entao
                  escreval ("maio")
               senao
                  se mes = 6 entao
                     escreval ("junho")
                  senao
                     se mes = 7 entao
                        escreval ("julho")
                     senao
                        se mes = 8 entao
                           escreval ("agosto")
                        senao
                           se mes = 9 entao
                              escreval ("setembro")
                           senao
                              se mes = 10 entao
                                 escreval ("outubro")
                              senao
                                 se mes = 11 entao
                                    escreval ("novembro")
                                 senao
                                    se mes = 12 entao
                                       escreval ("dezembro")
                                    senao
                                    fimse
                                 fimse
                              fimse
                           fimse
                        fimse
                     fimse
                  fimse
               fimse
            fimse
         fimse
      fimse
   fimse

Fimalgoritmo